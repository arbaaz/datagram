<html>
  <head>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
  </head>
  <body>
    <div class="container">
    <h1>Limo Cohort Analysis</h1>

    <table class="table table-striped table-condensed">
      <thead>
	<tr>
	  <th></th>
	  <th></th>
	  {% for d in series %}
	  <th>
	    {{d.signup_week}}
	  </th>
	  {% endfor %}
	</tr>
      </thead>
      {% for d in series %}
      <tr>
	<th style="background: #DC9EE6">
	  <a href="/api/v1/d/wVfv4got47jer7T7PHQVMw.html?params[format]=YYYY-MM&signup_period={{d.signup_week}}&refresh=600&views[]=table.jq&views[]=table.liquid">{{d.signup_week}}</a>
	</th>
	<td>{{ d.signups }}</td>
	{% for e in d.data %}
	<td>
	  {{ e.count }}
	</td>
	{% endfor %}
      </tr>
      {% endfor %}
    </table>
    </div>
  </body>
</html>
